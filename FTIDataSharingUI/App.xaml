<Application
    x:Class="FTIDataSharingUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="/Styles/FontSizes.xaml" />
                <ResourceDictionary Source="/Styles/Thickness.xaml" />
                <ResourceDictionary Source="/Styles/TextBlock.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="SegoeFluentIcons">ms-appx:/Assets/Fonts/SegoeIcons.ttf#Segoe Fluent Icons</FontFamily>
            <FontFamily x:Key="SegoeMDL2">ms-appx:/Assets/Fonts/segmdl2.ttf#Segoe MDL2 Assets</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
